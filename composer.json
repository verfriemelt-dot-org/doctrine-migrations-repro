{
    "require": {
        "php": "~8.5",
        "doctrine/doctrine-migrations-bundle": "^4.0",
        "symfony/console": "^8.0",
        "symfony/flex": "^2.10",
        "symfony/runtime": "^8.0",
        "symfony/http-foundation": "^8.0",
        "symfony/yaml": "^8.0",
        "doctrine/orm": "^3.6",
        "symfony/dotenv": "^8.0"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "\\App\\": "src"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
